{
  "manifest_version": 2,
  "name": "LangInject",
  "version": "1.0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content_mixed.js",
        "popup.js"
      ]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "storage",
    "<all_urls>"
  ],
  "web_accessible_resources": [
    "base.json"
  ]
}